# Test stderr of `log' when $terminal_output=no, $log=output.log, & logged
# command fails.

_testing="libprometheus/log"
_description="stderr, no terminal + file, fail"
_expect=pass

function _check {
	source "$_lib" || exit 1
	source "output.bash" || exit 1

	terminal_output=no
	logfile="output.log"
	> "$logfile"
	setup_logging

	log output "tnlycf stderr" 1 2> output.txt
	diff "tnlycf_stderr.txt" output.txt
}
