# Test stderr of `log' when $terminal_output=no, $log=output.log, & logged
# command passes.

_testing="libprometheus/log"
_description="stderr, no terminal + file, pass"
_expect=pass

function _check {
	source "$_lib" || exit 1
	source "output.bash" || exit 1

	terminal_output=no
	logfile="output.log"
	> "$logfile"
	setup_logging

	local out="$(log output "tnlycp stderr" 0 2>&1 1> /dev/null)"
	[[ -z "$out" ]]
}
