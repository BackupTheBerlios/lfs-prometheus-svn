# Test stderr of `log' when $terminal_output=yes, $logfile=output.log, & logged
# command fails.

_testing="libprometheus/log"
_description="stderr, terminal + file, fail"
_expect=pass

function _check {
	source "$_lib" || exit 1
	source "output.bash" || exit 1

	terminal_output=yes
	logfile="output.log"
	> "$logfile"
	setup_logging

	local out="$(log output "tylycf stderr" 1 2>&1 1> /dev/null)"
	[[ -z "$out" ]]
}
