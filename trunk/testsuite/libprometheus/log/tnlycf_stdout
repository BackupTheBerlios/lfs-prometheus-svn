# Test stdout of `log' when $terminal_output=no, $log=output.log, & logged
# command fails.

_testing="libprometheus/log"
_description="stdout, no terminal + file, fail"
_expect=pass

function _check {
	source "$_lib" || exit 1
	source "output.bash" || exit 1

	terminal_output=no
	logfile="output.log"
	setup_logging

	local out="$(log output "tnlycf stdout" 1)"
	[[ -z "$out" ]]
}
